{
  "endOfLine": "lf",
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "semi": true,
  "plugins": ["prettier-plugin-astro", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react", "^mobx", "^axios$", "^clsx$", "^dayjs$", "^qs$", "^uuid$", "^@?\\w", "^\\.", "^\\.\\./"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
